{
  "name": "duxingyu/repository",
  "description": "laravel repository",
  "type": "lib",
  "require": {
    "php": ">=5.4",
    "illuminate/support": "5.*",
    "illuminate/database": "5.*",
    "illuminate/filesystem": "5.*",
    "illuminate/console": "5.*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "duxingyu",
      "email": "530606930@qq.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Repository\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Repository\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Repository\\Providers\\RepositoryProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
